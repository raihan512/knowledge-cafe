  console.log("storedBookmark");
};

const getLocalStorage = () => {
  let bookmarks;
  const storedBookmark = localStorage.getItem("knowledge-bookmark");
  if (storedBookmark) {
    bookmarks = JSON.parse(storedBookmark);
  } else {
    bookmarks = {};
  }
  return bookmarks;
};

// export { setToDb, getLocalStorage };
export default setToDb;
